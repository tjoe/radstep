CREATE TABLE users (
	userid				INTEGER PRIMARY KEY,
	firstname			VARCHAR,
    lastname			VARCHAR,
    email				VARCHAR,
    username			VARCHAR,
    password			VARCHAR,
    confirmcode			VARCHAR
	);
	
